#
#  Makefile for wavy basic state linear model
 
OBJS= GFIDI.o DIFFUS.o SETSIG.o AMHMTM.o \
 GFRC.o SLNOUT.o  BLKDAT.o \
 BMCM.o	GLOOP.o COMINT.o \
 DEL4.o	DEL2.o DELLNP.o GSSTCD.o RESET.o \
 IMINV.o GETBS.o \
 WAVY.o DZTOUV.o GETFRC.o GETGEO.o \
 GETMAT.o SETFRC.o 
 
#
 
CFT=	cft77
LDR=	segldr
LDFLAGS=-L /lib,/usr/lib,/usr/local/lib,. -l imsl \
         .wavlib
LIBS=	
COMMAND=a.out
PROFLIB=
FFLAGS=
 
#

$(COMMAND):  $(OBJS)
	$(LDR) $(LDFLAGS) -o $(COMMAND) $(OBJS) $(LIBS)
 
#

WAVY.f:  WAVY.v1  ptrunk  pkmax  pimax  pjmax
	/lib/pcpp -P WAVY.v1 WAVY.f
GETMAT.f:  GETMAT.v1  ptrunk  pkmax
	/lib/pcpp -P GETMAT.v1 GETMAT.f
COMINT.f:  COMINT.v1  ptrunk  pimax  pjmax  pkmax  comcom  comsig  comctl \
  comphc  commnt  comtim  comcon  comnum  namcon  namttl
	/lib/pcpp -P COMINT.v1 COMINT.f 
GETBS.f:  GETBS.v1  comnum  comphc ptrunk  pkmax
	/lib/pcpp -P GETBS.v1 GETBS.f
GETGEO.f:  GETGEO.v1  pimax  pjmax  pkmax  ptrunk  comcom  comphc comnum
	/lib/pcpp -P GETGEO.v1 GETGEO.f
GFIDI.f:  GFIDI.v1  ptrunk  pimax  pjmax  pkmax  comctl  comphc \
  commnt  comtim  comnum
	/lib/pcpp -P GFIDI.v1 GFIDI.f
DIFFUS.f:  DIFFUS.v1  pkmax
	/lib/pcpp -P DIFFUS.v1 DIFFUS.f 
GLOOP.f:  GLOOP.v1  ptrunk  pimax  pjmax  pkmax  comcom  comnum \
  commnt  comtim  comphc
	/lib/pcpp -P GLOOP.v1 GLOOP.f
GFRC.f:  GFRC.v1  ptrunk  pimax  pjmax  pkmax  comctl  comphc \
  commnt  comtim  comnum
	/lib/pcpp -P GFRC.v1 GFRC.f
SETFRC.f:  SETFRC.v1  ptrunk  pimax  pjmax  pkmax  comcom  comnum \
  commnt  comtim  comphc
	/lib/pcpp -P SETFRC.v1 SETFRC.f
SLNOUT.f:  SLNOUT.v1  ptrunk  pimax  pjmax  pkmax  comnum comphc
	/lib/pcpp -P SLNOUT.v1 SLNOUT.f
AMHMTM.f:  AMHMTM.v1  comnum  comphc
	/lib/pcpp -P AMHMTM.v1 AMHMTM.f
BMCM.f:  BMCM.v1  comnum  comphc
	/lib/pcpp -P BMCM.v1 BMCM.f
DELLNP.f:  DELLNP.v1  comnum  comphc
	/lib/pcpp -P DELLNP.v1 DELLNP.f
DZTOUV.f:  DZTOUV.v1  comnum  comphc
	/lib/pcpp -P DZTOUV.v1 DZTOUV.f
IMINV.f:  IMINV.v1  comnum
	/lib/pcpp -P IMINV.v1 IMINV.f
RESET.f:  RESET.v1  comnum
	/lib/pcpp -P RESET.v1 RESET.f
SETSIG.f:  SETSIG.v1  comnum  comphc
	/lib/pcpp -P SETSIG.v1 SETSIG.f
DEL4.f:  DEL4.v1  comphc  comctl  comnum
	/lib/pcpp -P DEL4.v1 DEL4.f
DEL2.f:  DEL2.v1  comphc  comctl  comnum
	/lib/pcpp -P DEL2.v1 DEL2.f
GSSTCD.f:  GSSTCD.v1  ptrunk  pimax  pjmax  pkmax  comcom  comctl \
  comphc  comtim  comnum  commnt
	/lib/pcpp -P GSSTCD.v1 GSSTCD.f
BLKDAT.f:  BLKDAT.v1  comctl  comphc  comtim  commnt  comcon \
  comnum  pkmax  comsig  sigdef
	/lib/pcpp -P BLKDAT.v1 BLKDAT.f
GETFRC.f:  GETFRC.v1  comphc
	/lib/pcpp -P GETFRC.v1 GETFRC.f
#
 
clean:
	rm -f *.o *.l
 
clobber:  clean
	rm -f $(COMMAND)
 
void:  clobber
	rm -f $(SRCS) makefile
