#!/bin/sh
##########################################
# As ltime=60(1960) to 81(2011) see how K change
# independt of season(ss) and climate division
#########################################
set -eaux

lcdir=/cpc/home/wd52pp/project/prd_test/eeof
tmp=/cpc/home/wd52pp/tmp
datadir1=/cpc/home/wd52pp/data/LF_prd
datadir2=/cpc/home/wd52pp/data/prd_test
#
var=temp
nyr=82
nmon=989 #nyr(82)*12+mon(5)
#
/bin/rm $tmp/*.*         
cp $lcdir/runmean.102cd.f $tmp/runmean.f
cd $tmp
