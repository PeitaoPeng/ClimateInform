#!/bin/sh
#

set -eaux
   
datadir=/export/lnx169/wd52pp/data/trs/hgt

var=HGT500
   
syr=49
eyr=98
yr=$syr
while [ $yr -le $eyr ] ; do
   yrp=`expr $yr + 1`
 
filename=${var}_${yr}${yrp}_djfm_daily.LP
cat > $datadir/$filename.ctl << EOF
dset ^$filename.i3e
format yrev
options sequential
undef -9.99E+33
title BP daily
*
XDEF 144  LINEAR 0 2.5
*
YDEF 73 LINEAR -90. 2.5
*
ZDEF 01 LEVELS 1
*
TDEF 130 LINEAR 01JAN1975 1dy
*
*
VARS 1
z  01   99   hgt
ENDVARS
EOF

   yr=`expr $yr + 1`

done

exit
