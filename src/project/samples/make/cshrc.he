	set lpath = (/export/sgi80/fiorino/grads /usr/bin/X11 /usr/local/bin /var/sundesk/bin )
	set path = ( /usr/ucb /usr/lang $lpath /bin /usr/local ) 
	set path = ( $path /usr/bin /usr/etc /usr/games . ~ )
